: ${AWK=awk}

"$AWK" '
BEGIN { x0 = y0 = 1; a = 0.6; b = 0.4 }
NR == 1 { print; next }
NR == 2 { print; next }
{
    $1 = $1
    x = $2; y = $3
    if (ellipse(x, y, z)) $1 = "Water"; else $1 = "Air"
    print
}
function ellipse(x, y, z) {
    x -= x0; y -= y0
    return (x/a)^2 + (y/b)^2 < 1
}
' "$@"
